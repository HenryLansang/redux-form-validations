{
  "name": "redux-form-validations",
  "version": "0.0.1",
  "description": "Validation utilities for redux-form v6",
  "author": "PolicyGenius",
  "license": "MIT",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/policygenius/redux-form-validations.git"
  },
  "dependencies": {
    "eslint-config-policygenius": "^0.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "invariant": "^2.2.2",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "jest": "^20.0.4",
    "karma": "^1.7.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "modulePaths": [
      "./src"
    ],
    "testPathIgnorePatterns": [
      "./node_modules"
    ],
    "testRegex": "(\\.spec\\.js)",
    "setupTestFrameworkScriptFile": "./jest.setup.js",
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  },
  "scripts": {
    "clean": "rm -rf build/*",
    "lint": "eslint ./src --ext .js --ext .jsx",
    "release": "yarn run clean && babel src --out-dir build",
    "test:jest": "jest",
    "test:jest:debug": "jest --runInBand",
    "test:jest:watch": "jest --watch",
    "test:jest:coverage": "jest --coverage"
  }
}
